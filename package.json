  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "quotesy": "^1.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "eslint": "^7.25.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.3.8",
    "nodemon": "^2.0.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=8.10.0"
  }
